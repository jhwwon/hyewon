-- 모든 데이터 가져오기(dbeaver안에서는 기본으로는 200)
SELECT * FROM EMPLOYEES;   

-- salary컬럼의 값이 5000보다 큰 행 중에서 사원번호와 사원이름 값만 가져오기
SELECT employee_id, emp_name, salary
  FROM employees
 WHERE salary > 5000;

-- salary컬럼의 값이 5000보다 큰 행 중에서 사원번호와 사원이름 값만 가지고 오면서 employee_id의 값이 오름차순으로 조회
SELECT employee_id, emp_name, job_id
  FROM employees
 WHERE salary > 5000
 ORDER BY employee_id asc
;

-- AND조건 사용(조건식이 모두 맞아야 함)
SELECT employee_id, emp_name
  FROM employees
 WHERE salary > 5000
   AND job_id = 'IT_PROG'
 ORDER BY employee_id asc
;

-- OR조건 사용(조건식이 1개만 맞아도 조회가 가능(급여가 5000이거나 직무가 IT_PROG인 데이터))
SELECT employee_id AS ei, emp_name AS en, salary AS s, job_id AS ji1
  FROM employees
 WHERE salary > 5000
    OR job_id = 'IT_PROG'
 ORDER BY employee_id asc
;

-- insert ~ select 형태(INSERT INTO [스키마명].테이블명 (컬럼1, 컬럼2, ...))

CREATE TABLE ex3_2 (
	emp_id		NUMBER,
	emp_name	varchar2(100)
);

SELECT * FROM ex3_2;

-- insert ~ select로 데이터 삽입
INSERT INTO ex3_2(emp_id, emp_name) 
SELECT employee_id, emp_name FROM employees WHERE salary > 5000;

-- 수식연산자(+,-,/,*)
SELECT 1 + 1 AS plus1 FROM DUAL;
SELECT 3 / 2 AS divide1 FROM DUAL;
SELECT 3 * 7 AS multiply1 FROM dual;
SELECT 1+1 AS plus1, 3/2 AS divide1 FROM DUAL;
SELECT "1" + "1" AS plus1 FROM dual;  -- error
SELECT '1' + '1' AS plus1 FROM dual;  -- concat로 실행가능(11)

-- 문자연산자(||)
SELECT '1' || '1' AS concat1 FROM dual;  -- (11)

SELECT employee_id || '-' || emp_name AS employee_info
  FROM employees
 WHERE ROWNUM < 5
;

SELECT employee_id + '-' + emp_name AS employee_info
  FROM employees
 WHERE ROWNUM < 5
;

SELECT * FROM employees;

-- 논리연산자(>,<,>=,<=,<>(같지 않다),!=(같지 않다),^=(같지 않다))
-- != 예제
SELECT employee_id, emp_name
  FROM EMPLOYEES
 WHERE email = 'WSMITH'
;

-- 표현식(CASE WHEN ~ END AS 이름)
-- 사원 테이블에서 각 사원의 급여에 따라 5000이하로 급여를 받는 사원 C
--                              5000 ~ 10000이하로 급여를 받는 사원 B
--                              15000이상로 급여를 받는 사원 A
SELECT employee_id, 
       salary,
       CASE WHEN salary <= 5000 THEN 'C등급'
            WHEN salary > 5000 AND salary <= 15000 THEN 'B등급'
            -- WHEN salary >= 15000 THEN 'A등급'
            ELSE 'A등급'
       END AS salary_grade
  FROM employees
 WHERE salary >= 3000
   -- AND EMPLOYEE_ID = 106
   -- AND salary_grade = 'A등급'  -- error
;

SELECT employee_id, 
       salary,
       CASE WHEN salary <= 5000 THEN 3
            WHEN salary > 5000 AND salary <= 15000 THEN 2
            -- WHEN salary >= 15000 THEN 'A등급'
            ELSE 1
       END AS salary_grade
  FROM employees
 WHERE salary >= 3000
   -- AND EMPLOYEE_ID = 106
   -- AND salary_grade = 'A등급'  -- error
;

-- chapter3 문제1번
CREATE TABLE ex3_6 (
	employee_id		number(6),
	emp_name		varchar2(80),
	salary   		number(8,2),
	manager_id		number(6)
);

INSERT INTO ex3_6(employee_id, emp_name, salary, manager_id) 
SELECT employee_id, emp_name, salary, manager_id
  FROM employees
 WHERE manager_id = 124
   AND salary >= 2000 AND salary <= 3000
   --AND salary BETWEEN 200 AND 3000
;

SELECT * FROM ex3_6; 


-- any 비교 조건식('or' 여러 개인 것(급여가 2000이거나 3000이거나 4000인 것))
SELECT employee_id, salary
  FROM employees
 WHERE salary = any(2000, 3000, 4000)
 ORDER BY employee_id ASC
;

SELECT employee_id, salary
  FROM employees
 WHERE salary = 2000
    OR salary = 3000
    OR salary = 4000
 ORDER BY employee_id ASC
;


-- all 비교 조건식('and' 여러 개인 것(급여가 2000이고 3000이고 4000인 것))
SELECT employee_id, salary
  FROM employees
 WHERE salary = all(2000, 3000, 4000)
 ORDER BY employee_id ASC
;

SELECT employee_id, salary
  FROM employees
 WHERE salary = 2000
   and salary = 3000
   and salary = 4000
 ORDER BY employee_id ASC
;

-- some 비교 조건식(any조건식과 동일)
SELECT employee_id, salary
  FROM employees
 WHERE salary = some(2000, 3000, 4000)
 ORDER BY employee_id ASC
;

-- NOT 논리 조건식
SELECT employee_id, salary
  FROM employees
 WHERE NOT(salary >= 2500)
 ORDER BY employee_id ASC
;

SELECT employee_id, salary
  FROM employees
 WHERE NOT(employee_id = 171)
   AND NOT(salary >= 3000)
 ORDER BY employee_id ASC
;

SELECT * FROM employees;

-- NULL 조건식(IS 연산자를 사용)
SELECT employee_id, emp_name 
  FROM employees
 WHERE commission_pct is null
;

SELECT employee_id, emp_name 
  FROM employees
 WHERE commission_pct is not null
;

-- between 조건식
SELECT employee_id, salary
  FROM employees
 WHERE salary BETWEEN 2000 AND 2500
 ORDER BY employee_id
;

-- in 조건식(or를 여러개 한거와 같음(any, some과 같음))
SELECT employee_id, salary
  FROM employees
 WHERE salary IN (2000, 3000, 4000)
;

SELECT employee_id, salary
  FROM employees
 WHERE salary = 2000
    OR salary = 3000
    OR salary = 4000
;

SELECT employee_id, salary
  FROM employees
 WHERE salary NOT IN (2000, 3000, 4000)
;

-- chapter3장 3번 문제 
-- 사원테이블(employees)에서 커미션(commission_pct) 값이 없는 사원의 사번과 사원명을 추출하는 쿼리를 작성해보자.
SELECT employee_id, emp_name
  FROM employees
 WHERE commission_pct = ''
    OR commission_pct IS NULL
;

-- chapter3장 4번 문제
-- 아래의 쿼리를 논리연산자로 변환해보자. 
SELECT employee_id, salary 
  FROM employees
WHERE salary BETWEEN 2000 AND 2500
ORDER BY employee_id;

SELECT employee_id, salary 
  FROM employees
WHERE salary >= 2000 AND salary <= 2500
ORDER BY employee_id;


-- chapter3장 5번 문제
-- 다음의 두 쿼리를 ANY, ALL을 사용해서 동일한 결과를 추출하도록 변경해보자.
SELECT employee_id, salary 
  FROM employees
WHERE salary IN (2000, 3000, 4000)
ORDER BY employee_id;

SELECT employee_id, salary 
  FROM employees
WHERE salary = ANY (2000, 3000, 4000)
ORDER BY employee_id;



SELECT employee_id, salary 
  FROM employees
WHERE salary NOT IN (2000, 3000, 4000)
ORDER BY employee_id;

SELECT employee_id, salary 
  FROM employees
WHERE salary != ALL(2000, 3000, 4000)
ORDER BY employee_id;

DROP TABLE departments;

-- department부서테이블 생성
CREATE TABLE departments(
	department_id    		number(6,0)      PRIMARY KEY,    -- 부서번호
	department_name  		varchar2(80)	 NOT NULL,       -- 부서이름
	manager_id   			number(6,0),                      -- 부서장의 employee_id
	parent_id               number(6,0),					 -- 상위부서 번호
	create_date      		DATE          DEFAULT sysdate,    -- 데이터 생성날짜
	-- 데이터 수정날짜
	update_date      		DATE          DEFAULT sysdate   
);

COMMENT ON COLUMN departments.department_id IS '부서번호';
COMMENT ON COLUMN departments.department_name IS '부서이름';
COMMENT ON COLUMN departments.manager_id IS '부서장의 employee_id';
COMMENT ON COLUMN departments.parent_id IS '상위부서 번호';
COMMENT ON COLUMN departments.create_date IS '데이터 생성날짜';
COMMENT ON COLUMN departments.update_date IS '데이터 수정날짜';

-- DDL - Data Definition(정의) Language    -> 스키마(Scheme) 생성 및 변경(CREATE, ALTER, DROP)
-- DML - Data Manipulation(조작) Language  -> 데이터 생성 및 변경(SELECT, INSERT)
-- DCL - Data Control(제어) Language       -> DB 권한 생성 및 변경(GRANT)

