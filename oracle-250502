-- Oracle SELECT 전문가처럼 사용하기
--    SELECT *(모든 컬럼) 혹은 컬럼1, 컬럼2, ... 
--      FROM [스키마명].테이블명 혹은 [스키마명].뷰명)
--     WHERE 조건1, 조건2, ...
--  ORDER BY 컬럼1, 컬럼2, ...

-- 사원 정보 데이터를 담은 테이블

DROP TABLE employees;

CREATE TABLE employees(
	employee_id		 number(6,0) 	PRIMARY KEY,	   -- 사원번호
	emp_name		 varchar2(80)	NOT NULL,	       -- 사원명
	email		  	 varchar2(100),				       -- 이메일
	phone_number	 varchar2(20),                     -- 전화번호
	hire_date        date           NOT NULL,          -- 입사일자
	salary           number(8, 2),                     -- 급여
	manager_id       number(6,0),                      -- 상위 관리자 번호
	commission_pct   NUMBER(2,2),                      -- 커미션율
	retire_date      date,                             -- 퇴사일자
	department_id    number(6,0),                      -- 소속된 부서번호
	job_id           varchar2(10),                     -- 직무id
	create_date      DATE          DEFAULT sysdate,    -- 데이터 생성날짜
	-- 데이터 수정날짜
	update_date      DATE          DEFAULT sysdate     
);

COMMENT ON COLUMN EMPLOYEES.employee_id IS '사원번호';
COMMENT ON COLUMN EMPLOYEES.emp_name IS '사원명';
COMMENT ON COLUMN EMPLOYEES.email IS '이메일';
COMMENT ON COLUMN EMPLOYEES.phone_number IS '전화번호';
COMMENT ON COLUMN EMPLOYEES.hire_date IS '입사일자';
COMMENT ON COLUMN EMPLOYEES.salary IS '급여';
COMMENT ON COLUMN EMPLOYEES.manager_id IS '상위 관리자 번호';
COMMENT ON COLUMN EMPLOYEES.commission_pct IS '커미션율';
COMMENT ON COLUMN EMPLOYEES.retire_date IS '상위 관리자 번호';
COMMENT ON COLUMN EMPLOYEES.department_id IS '소속된 부서번호';
COMMENT ON COLUMN EMPLOYEES.job_id IS '직무id';
COMMENT ON COLUMN EMPLOYEES.create_date IS '데이터 생성날짜';
COMMENT ON COLUMN EMPLOYEES.update_date IS '데이터 수정날짜';

