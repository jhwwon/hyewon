SELECT * FROM departments;
SELECT * FROM EMPLOYEES;

-- exists조건식 사용(임시 테이블 정보의 데이터가 있는지 체크해주는 조건식)
-- 급여가 3000이상을 받은 직원이 있는 부서의 이름 조회
SELECT department_id, department_name
  FROM departments a
 WHERE exists(SELECT *
 				FROM employees b 
 			   WHERE a.DEPARTMENT_ID = b.department_id 
 			     AND b.salary > 3000
 	   )
ORDER BY a.department_name
;

SELECT * FROM EMPLOYEES e;

SELECT department_id, department_name
  FROM departments a
 WHERE exists(SELECT employee_id FROM employees WHERE employee_id = 0)
;


-- LIKE 조건식(직원 이름이 'Al'로 시작하는 모든 직원 데이터 조회.  '%'는 모든 글자들)
SELECT emp_name
  FROM employees
 WHERE emp_name LIKE 'Al%'
 ORDER BY emp_name
;

-- LIKE 조건식(직원 이름이 'z'로 끝나는 모든 직원 데이터 조회.  '%'는 모든 글자들)
SELECT emp_name
  FROM employees
 WHERE emp_name LIKE '%z'
 ORDER BY emp_name
;

-- LIKE 조건식(직원 이름이 중간에 'b'가 있는 모든 직원 데이터 조회.  '%'는 모든 글자들)
SELECT emp_name
  FROM employees
 WHERE emp_name LIKE '%t%'
 ORDER BY emp_name
;

CREATE table ex3_5(names varchar2(30));

-- LIKE 조건식(밑줄(_)은 한 글자만 비교하기 위한 용도))
INSERT INTO ex3_5 VALUES ('홍길');
INSERT INTO ex3_5 VALUES ('홍길동');
INSERT INTO ex3_5 VALUES ('홍길용');
INSERT INTO ex3_5 VALUES ('홍길상');
INSERT INTO ex3_5 VALUES ('홍길상동');

SELECT * FROM ex3_5 WHERE names LIKE '홍길%'; -- '홍길'을 포함한 2글자의 이상의 글자들
SELECT * FROM ex3_5 WHERE names LIKE '홍길_'; -- '홍길'로 시작하는 글자의 3글자만

